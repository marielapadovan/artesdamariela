  title = {Environment-Aware Virtual Slice Provisioning in Green Cloud Environment},
  journal = {IEEE Transactions on Services Computing},
  year = {2015},
  volume = {8},
  number = {3},
  pages = {507--519},
  doi = {10.1109/TSC.2014.2362544}
}
