  title = {3d Stacked Dram Refresh Management With Guaranteed Data Reliability},
  journal = {IEEE Transactions On Computer-aided Design Of Integrated Circuits And Systems},
  year = {2015},
  volume = {Pp},
  number = {99},
  pages = {1--1},
