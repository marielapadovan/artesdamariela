  title = {A Scalable, Commodity Data Center Network Architecture},
  booktitle = {Proceedings of the ACM SIGCOMM 2008 Conference on Data Communication},
  publisher = {ACM},
  year = {2008},
  pages = {63--74},
  url = {http://doi.acm.org/10.1145/1402958.1402967},
