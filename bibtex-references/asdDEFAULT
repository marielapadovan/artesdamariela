  title = {Hardware Support For Accurate Per-task Energy Metering In Multicore Systems},
  journal = {ACM Transactions On Architecture And Code Optimization},
  year = {2013},
  volume = {10},
  number = {4},
  pages = {341--3427},
  doi = {10.1145/2541228.2555291}
}
