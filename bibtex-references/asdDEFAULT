  title = {Implementation And Performance Analysis Of Various Vm Placement Strategies In Cloudsim},
  journal = {Journal Of Cloud Computing},
  year = {2015},
  volume = {4},
  number = {1},
  pages = {20},
  doi = {10.1186/s13677-015-0045-5}
}
</pre></td>
