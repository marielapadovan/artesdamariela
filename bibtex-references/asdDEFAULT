  title = {Life Cycle Cost Analysis Of Data Center Project},
  booktitle = {2014 Ninth International Conference On Ecological Vehicles And Renewable Energies (ever)},
  year = {2014},
  pages = {1--6},
  doi = {10.1109/EVER.2014.6844139}
}
</pre></td>
